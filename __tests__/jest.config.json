{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "modulePaths": ["<rootDir>"],
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "coverageDirectory": "./coverage",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleDirectories": ["<rootDir>/../", "node_modules"]
}
